{
  "extends": "@acme/tsconfig/base.json",
  "exclude": [],
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.ts",
    "**/*.tsx",
    "../src/**/*.ts",
    "../src/**/*.tsx",
    "next.config.mjs",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@payload-config": ["./payload.config.ts"],
      "@acme/payload-plugin-template": [
        "../../payload-plugins/template/src/index.ts"
      ],
      "@acme/payload-plugin-template/client": [
        "../../payload-plugins/template/src/exports/client.ts"
      ],
      "@acme/payload-plugin-template/rsc": [
        "../../payload-plugins/template/src/exports/rsc.ts"
      ]
    },
    "noEmit": true,
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
