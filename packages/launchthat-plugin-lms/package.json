{
  "name": "launchthat-plugin-lms",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./convex/component": {
      "types": "./dist/convex/component/index.d.ts",
      "default": "./src/convex/component/index.ts"
    },
    "./convex/component/convex.config": {
      "types": "./dist/convex/component/convex.config.d.ts",
      "default": "./src/convex/component/convex.config.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
