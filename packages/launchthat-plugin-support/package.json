{
  "name": "launchthat-plugin-support",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./settings": {
      "types": "./dist/settings.d.ts",
      "default": "./src/settings.ts"
    },
    "./assistant": {
      "types": "./src/assistant/experiences.ts",
      "default": "./src/assistant/experiences.ts"
    },
    "./assistant/openai": {
      "types": "./src/assistant/openai.ts",
      "default": "./src/assistant/openai.ts"
    },
    "./convex/component": {
      "types": "./dist/convex/component/index.d.ts",
      "default": "./src/convex/component/index.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@acme/admin-runtime": "workspace:*",
    "@acme/ui": "workspace:*",
    "@acme/ui-lexical": "workspace:*",
    "@ai-sdk/react": "^3.0.3",
    "@convex-dev/presence": "^0.3.0",
    "ai": "^6.0.3",
    "convex": "^1.23.0",
    "dev": "^0.1.3",
    "framer-motion": "^12.12.2",
    "launchthat-plugin-core": "workspace:*",
    "lucide-react": "^0.555.0",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-resizable-panels": "^2.1.8"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:react19",
    "@types/react-dom": "catalog:react19",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "next": "catalog:"
  },
  "prettier": "@acme/prettier-config"
}
