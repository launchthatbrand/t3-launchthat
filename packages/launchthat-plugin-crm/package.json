{
  "name": "launchthat-plugin-crm",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./convex/component": {
      "types": "./dist/convex/component/index.d.ts",
      "default": "./src/convex/component/index.ts"
    },
    "./convex/component/convex.config": {
      "types": "./dist/convex/component/convex.config.d.ts",
      "default": "./src/convex/component/convex.config.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "@acme/ui": "workspace:*",
    "dev": "^0.1.3",
    "launchthat-plugin-core": "workspace:*",
    "lucide-react": "^0.555.0"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/react": "catalog:react19",
    "convex": "^1.23.0",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "react": "catalog:react19",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "convex": "^1.23.0",
    "react": "catalog:react19"
  },
  "prettier": "@acme/prettier-config"
}
