{
  "extends": "@acme/tsconfig/compiled-package.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "moduleResolution": "bundler",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "paths": {
      "@acme/ui": ["../ui/src/index.ts"],
      "@acme/ui/*": ["../ui/src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}

