{
  "name": "launchthat-plugin-discord",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./convex/component": {
      "types": "./dist/convex/component/index.d.ts",
      "default": "./src/convex/component/index.ts"
    },
    "./convex/component/convex.config": {
      "types": "./dist/convex/component/convex.config.d.ts",
      "default": "./src/convex/component/convex.config.ts"
    },
    "./runtime": {
      "types": "./dist/runtime/index.d.ts",
      "default": "./src/runtime/index.ts"
    },
    "./runtime/crypto": {
      "types": "./dist/runtime/crypto.d.ts",
      "default": "./src/runtime/crypto.ts"
    },
    "./runtime/discordApi": {
      "types": "./dist/runtime/discordApi.d.ts",
      "default": "./src/runtime/discordApi.ts"
    },
    "./runtime/oauth": {
      "types": "./dist/runtime/oauth.d.ts",
      "default": "./src/runtime/oauth.ts"
    },
    "./runtime/credentials": {
      "types": "./dist/runtime/credentials.d.ts",
      "default": "./src/runtime/credentials.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "convex": "^1.23.0",
    "launchthat-plugin-core": "workspace:*"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:*",
    "@acme/prettier-config": "workspace:*",
    "@acme/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "next": "catalog:"
  },
  "prettier": "@acme/prettier-config"
}
